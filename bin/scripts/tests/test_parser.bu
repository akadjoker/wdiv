// test_parser.bu
var x = 1 + 2 * 3 / 4 - 5 % 2;  // Precedência
assert_eq(x, 1, "operator precedence");

var s = "a\"b\\n\"c";  // Escapes
assert_eq(s.length(), 7, "string escapes");


assert_eq(2 + 3 * 4, 14, "mul_before_add");     // 2 + 12
assert_eq((2 + 3) * 4, 20, "parentheses");      // 5 * 4  
assert_eq(10 / 2 * 3, 15, "div_mul_left");      // (5 * 3)
assert_eq(10 % 3, 1, "modulo");
assert_eq(1 + 2 * 3 / 4 - 5 % 2, 1, "full_prec");  // COMPLETO ✓

pass("parser precedence");
